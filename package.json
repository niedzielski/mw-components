{
  "name": "@wikimedia/mw-components",
  "author": "Wikimedia",
  "description": "Vue.js user interface components for Wikipedia, MediaWiki, and beyond.",
  "version": "0.0.0",
  "keywords": [
    "Vue.js",
    "components",
    "Wikipedia",
    "MediaWiki",
    "Wikimedia",
    "TypeScript"
  ],
  "homepage": "https://mw-components.netlify.app/ui",
  "repository": "github:niedzielski/mw-components",
  "bugs": "https://phabricator.wikimedia.org/tag/vue.js",
  "license": "MIT",
  "main": "dist/mwc.js",
  "scripts": {
    "start": "start-storybook -p 6006 --quiet",
    "build": "webpack -p",
    "test": "npm -s run test:unit && npm -s run build && npm -s run test:size && npm -s run test:lint:js && npm -s run test:lint:styles && npm -s run docs",
    "test:unit": "jest -c .jest/jest.config.json",
    "test:size": "bundlesize",
    "test:lint:js": "npm -s run linter:js -- .",
    "test:lint:styles": "npm -s run linter:styles -- .",
    "format": "npm -s run linter:js -- --fix . && npm -s run linter:styles -- --fix .",
    "linter:js": "eslint --cache --max-warnings 0 --report-unused-disable-directives --ext .js,.json,.ts,.vue",
    "linter:styles": "stylelint --cache --rd --mw 0",
    "docs": "npm -s run docs:ui && npm -s run docs:sourceMap:mwcMin && npm -s run docs:sourceMap:mwcMinGzip && npm -s run docs:sourceMap:commonMin && npm -s run docs:sourceMap:commonMinGzip && npm -s run docs:sourceMap:primitivesMin && npm -s run docs:sourceMap:primitivesMinGzip && npm -s run docs:sourceMap:searchMin && npm -s run docs:sourceMap:searchMinGzip && npm -s run docs:sourceMap:mediaWikiUiButtonMin && npm -s run docs:sourceMap:mediaWikiUiButtonMinGzip && npm -s run test:size|grep dist > docs/minGzipBundleSize.txt",
    "docs:ui": "build-storybook --quiet --loglevel warn -o docs/ui",
    "docs:sourceMap:mwcMin": "source-map-explorer                          --html docs/sourceMaps/mwcMin.html                   dist/mwc.{js,css}",
    "docs:sourceMap:mwcMinGzip": "source-map-explorer               --gzip --html docs/sourceMaps/mwcMinGzip.html               dist/mwc.{js,css}",
    "docs:sourceMap:commonMin": "source-map-explorer                       --html docs/sourceMaps/commonMin.html                dist/mwc-common.{js,css}",
    "docs:sourceMap:commonMinGzip": "source-map-explorer            --gzip --html docs/sourceMaps/commonMinGzip.html            dist/mwc-common.{js,css}",
    "docs:sourceMap:primitivesMin": "source-map-explorer                   --html docs/sourceMaps/primitivesMin.html            dist/mwc-primitives.{js,css}",
    "docs:sourceMap:primitivesMinGzip": "source-map-explorer        --gzip --html docs/sourceMaps/primitivesMinGzip.html        dist/mwc-primitives.{js,css}",
    "docs:sourceMap:searchMin": "source-map-explorer                       --html docs/sourceMaps/searchMin.html                dist/mwc-search.{js,css}",
    "docs:sourceMap:searchMinGzip": "source-map-explorer            --gzip --html docs/sourceMaps/searchMinGzip.html            dist/mwc-search.{js,css}",
    "docs:sourceMap:mediaWikiUiButtonMin": "source-map-explorer            --html docs/sourceMaps/mediaWikiUiButtonMin.html     dist/mediawiki.ui.button.css",
    "docs:sourceMap:mediaWikiUiButtonMinGzip": "source-map-explorer --gzip --html docs/sourceMaps/mediaWikiUiButtonMinGzip.html dist/mediawiki.ui.button.css",
    "preversion": "[ -z \"$(git status -z)\" ]",
    "postversion": "git push --follow-tags origin HEAD && npm publish",
    "prepublishOnly": "npm -s t"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@storybook/addon-a11y": "6.0.0-beta.24",
    "@storybook/addon-actions": "6.0.0-beta.24",
    "@storybook/addon-backgrounds": "5.3.18",
    "@storybook/addon-knobs": "6.0.0-beta.24",
    "@storybook/addon-links": "6.0.0-beta.24",
    "@storybook/addon-storysource": "6.0.0-beta.24",
    "@storybook/addon-viewport": "6.0.0-beta.24",
    "@storybook/vue": "6.0.0-beta.24",
    "@types/jest": "25.2.2",
    "@types/jquery": "3.3.38",
    "@types/mini-css-extract-plugin": "0.9.1",
    "@types/optimize-css-assets-webpack-plugin": "5.0.1",
    "@types/terser-webpack-plugin": "2.2.0",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "@vue/test-utils": "1.0.3",
    "babel-core": "6.26.3",
    "babel-preset-vue": "2.0.2",
    "bundlesize": "0.18.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.5.3",
    "eslint": "6.8.0",
    "eslint-config-wikimedia": "0.15.3",
    "eslint-plugin-jest": "23.13.1",
    "fork-ts-checker-webpack-plugin": "4.1.4",
    "jest": "26.0.1",
    "less-loader": "6.1.0",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pre-commit": "1.2.2",
    "source-map-explorer": "2.4.2",
    "stylelint": "15.10.1",
    "stylelint-config-wikimedia": "0.10.1",
    "terser-webpack-plugin": "3.0.1",
    "ts-jest": "26.0.0",
    "ts-loader": "7.0.4",
    "typescript": "3.9.2",
    "vue": "2.6.11",
    "vue-jest": "3.0.5",
    "vue-loader": "15.9.2",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "wikimedia-ui-base": "0.15.0"
  }
}
